// 头部下拉菜单
.el-dropdown-link{
    cursor: pointer;
    .el-icon--right {
      margin-left: 0px;
    }
}
.el-dropdown-menu{ //旅馆信息下拉
    width: 300px;
    border-radius: 0 !important;
    background-color: #f3f3f3 !important;
    padding: 5px 0;
    margin: 0px 0;
    .el-dropdown-menu__item{
        color: #666 !important;
        font-size: 14px;
        line-height: 44px;
        margin: 0;
        padding: 0 12px;
        text-align: center;
        &.btn{
          border-top: 1px solid #B2B2B2;
        }
        .menu{
          >div{
            display: inline-block;
            font-size: 12px;
            color: #000000;
            i{
              color: #0d61b6;
              font-size: 14px;
              margin-left: 0px;
            }
          }
          >label{
            font-size: 12px;
            color: #787878; 
          }
          >span{
            font-size: 12px;
            color: #000000;
          }
        }
        .menu-phone{
          display: flex;
          >label{
            font-size: 12px;
            color: #787878; 
            min-width: 66px;
            // flex: 1
          }
          >div{
            font-size: 12px;
            color: #000000;
            >p{
              &.icon{
                display: inline-block;
                padding-left: 20px;
                &.icon-tel{
                  position: relative;
                  background: url('../../image/common/icon-tel.png') 0px 4px no-repeat;
                  background-size: 14px;
                }
                &.icon-mobile{
                  padding-top: 5px;
                  background: url('../../image/common/icon-mobile.png') 0px 7px no-repeat;
                  background-size: 16px;
                }
              }
            }
          }
        }
        >.icon{
          display: inline-block;
          padding-left: 30px;
          font-style: normal;
          &.icon-change{
            background: url('../../image/common/checkout-g.png') 5px center no-repeat;
            background-size: 15px;
          }
          &.icon-changepwd{
            background: url('../../image/common/changepwd-g.png') 5px center no-repeat;
            background-size: 16px;
          }
          &.icon-unit{
            background: url('../../image/common/unit-g.png') 5px center no-repeat;
            background-size: 15px;
          }
        }
        &.is-disabled{
            font-size: 13px;
            line-height: 20px;
            margin: 8px 0;
            pointer-events: none;
            cursor: default;
            text-align: left;
        }
        &.el-dropdown-menu__item--divided{
            color: #666 !important;
            font-size: 14px;
            line-height: 44px;
            margin: 0;
            padding: 0 20px;
            &:before{
                height: 1px;
                margin: 0 -14px;
            }
        }
        &:hover{
          background-color: #0d61b6 !important;
          color:#fff !important;
          >.icon{
            &.icon-change{
              background: url('../../image/common/checkout-w.png') 5px center no-repeat;
              background-size: 15px;
            }
            &.icon-changepwd{
              background: url('../../image/common/changepwd-w.png') 5px center no-repeat;
              background-size: 16px;
            }
            &.icon-unit{
              background: url('../../image/common/unit-w.png') 5px center no-repeat;
              background-size: 15px;
            }
          }
        }
    }
    .popper__arrow{
      display: none;
    }
}
.el-menu--horizontal{ //菜单下拉列表
    .el-menu--popup-bottom-start {
        margin-top: 0px;
        box-shadow: 2px -1px 2px 1px #dedede !important;
    }
    .el-menu--popup{
        min-width: 150px;
        background-color: #d3d3d3 !important;
        padding: 0;
        .el-menu-item {
            background-color: #e9e9e9 !important;
            margin-bottom: 1px;
            padding: 0 20px;
            &.is-active{
                color: #0d61b6 !important;
            }
            &:hover{
                color: #0d61b6 !important;
            }
        }
    }
}

/************内宾登记**********/
.label {
    display: inline-block;
    margin: 0px 15px 10px 0;
    font-size: 16px;
    font-weight: 500;
}
.lgy_child_content {
  .label{
    margin: 5px 10px 15px 0;
  }
  .refresh{
    margin-top: 7px;
  }
  &.child-chineseReg{
    .pagination {
      padding: 12px 0;
      height: 56px;
    }
  }
}
.refresh{
  float: right;
  margin-top: 2px;
  font-size: 13px;
  font-weight: 500;
  cursor: pointer;
  padding-left: 20px;
  background: url('../../image/common/refresh.png') no-repeat 5px center;
  background-size: 15px;
  color: #0D61B6;
  border: 1px solid #0d61b6;
  border-radius: 4px;
  padding: 4px 5px 4px 26px;
}
.refreshf{
    float: right;
    font-size: 13px;
    font-weight: 500;
    cursor: pointer;
    margin-right: 14px;
    background-size: 15px;
    color: #0d61b6;
    margin-top: 4px;
    border: 1px solid #0d61b6;
    border-radius: 4px;
    padding: 4px 5px;
}
.refreshf:hover, .refresh:hover{
    background-color: rgba(13,97,182,0.2);
    transition: background-color 0.4s;
    -webkit-transition: background-color 0.4s;
}
.todayrecord{
  font-size: 14px;
  font-weight: 500;
  color: #0D61B6;
  margin-left: 40px;
  text-decoration: underline;
  cursor: pointer;
}

.el-table td, .el-table th{
  padding: 0;
}

.lgy_child_content {
    .reg-record {
        border: 1px solid #D6D6D6;
        // margin-right: 20px;
        -webkit-box-shadow: 0px 1px 4px rgba(161, 161, 161, 0.431);
        box-shadow: 0px 1px 4px rgba(161, 161, 161, 0.431);
        .el-table__header-wrapper,.el-table__fixed-header-wrapper {
          .is-leaf{
            background-color: #FFF8C7;
            height: tableThHeight;
            line-height: tableThHeight;
            padding: 0;
            font-size: 12px;
            color: #666;
            .cell{
              line-height: 40px;
            }
          }
        }
        
        .el-table__body-wrapper, .el-table__fixed, .el-table__fixed-right {
          tr {
            &.el-table__row--striped {
              td {
                background-color: #F4F4F4;
              }
            }
            &.current-row {
              td {
                background-color: #0D61B6 !important;
                color: #fff;
                .pass {
                  display: block;
                  height: 30px;
                  background: url('../../image/common/pass-w.png') no-repeat center;
                  background-size: 15px;
                }
                .nopass {
                  display: block;
                  height: 30px;
                  background: url('../../image/common/notpass-w.png') no-repeat center;
                  background-size: 15px;
                }
              }
            }
            td {
              height: tableTdHeight;
              line-height: tableTdHeight;
              padding: 0;
              font-size: 13px;
              font-family: "微软雅黑";
              color: #454545;
              cursor: pointer;
            }
          }
        }
    }
    .pagination {
      position: relative;
      width: 100%;
      box-sizing: border-box;
      padding: 5px 0;
      height: 45px;
      &.visitReg{
        margin: 7px 0;
      }
      .el-pagination {
          display: inline-block;
          // position: relative;
          font-size: 15px;
          float: right;
          ::before, ::after {
              clear: both;
          }
      }
      .el-pagination__total {
          position: absolute;
          font-size: 14px;
          left: 20px;
      }
      .el-pager {
          padding: 4px 0;
          .more::before {
              line-height: 22px;
          }
          li {
              margin: 0 4px;
              color: #666;
              border-radius: 5px;
              background: #fff;
              font-size: 14px;
              min-width: 20px;
              height: 20px;
              line-height: 20px;
              -webkit-box-sizing: border-box;
              box-sizing: border-box;
              &.active {
                  border-radius: 10px;
                  background-color: #0D61B6;
                  color: #fff;
              }
          }
      }
      span {
        position: relative;
        font-size: 14px;
      }
      .page-size {
        float: right;
        margin-right: 30px;
        line-height: 35px;
        display: inline-block;
        height: 35px;
      }
    }
}

.reg-detail {
  margin-left: 20px;
