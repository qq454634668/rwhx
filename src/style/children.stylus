tableThHeight = 40px;
tableTdHeight = 44px;

.lgy-blue{
  background-color: #0d61b6 !important;
  border-color: #0d61b6 !important;
}
.lgy-container{
   .el-button--primary,.el-button--primary.is-active, .el-button--primary:active, .el-button--primary:focus, .el-button--primary:hover {
    background: #0d61b6;
    border-color: #0d61b6;
    color: #fff;
  }
}
.icon-xiala{
  font-size: 12px;
